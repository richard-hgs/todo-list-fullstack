{
  "name": "todo-list-fullstack",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist tsconfig.build.tsbuildinfo",
    "build": "npm-run-all -s nest:build cpy:assets",
    "nest:build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:migrate-dev": "npx prisma migrate dev",
    "prisma:migrate-rollback": "npx prisma migrate resolve --rolled-back \"20241210165100_migration_name\"",
    "prisma:generate": "npx prisma generate",
    "prisma:format": "npx prisma format",
    "prisma:seed": "ts-node prisma/seed.ts",
    "mailhog:dev": "tools\\mailhog.exe",
    "mailhog:dev2": "tools/mailhog.exe",
    "cpy:assets": "copy-and-watch \"src/**/*.{html,css,json}\" \"dist\"",
    "cpy:assets-watch": "copy-and-watch --watch --skip-initial-copy \"src/**/*.{html,css,json}\" \"dist\"",
    "deps:audit-size": "yarn --ignore-scripts --audit",
    "deps:audit": "yarn audit",
    "deps:audit-fix": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.4",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express": "^4.21.1",
    "handlebars": "^4.7.8",
    "moment": "^2.30.1",
    "nestjs-i18n": "^10.4.9",
    "nestjs-prisma": "^0.23.0",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "response-time": "^2.3.3",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/testing": "^10.4.4",
    "@nestjs/typeorm": "^10.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.13",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.5",
    "@types/nodemailer": "^6.4.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/response-time": "^2.3.8",
    "@types/supertest": "^6.0.2",
    "copy-and-watch": "^0.1.8",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.22.0",
    "recursive-copy-cli": "^1.0.20",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/tests/**"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./utils/tests/jest-setup.ts"
    ]
  }
}
